{
    "name": "radiocubito/laravel-blade-input",
    "description": "Laravel blade input components made with Tailwind CSS",
    "keywords": [
        "radiocubito",
        "laravel-blade-input"
    ],
    "homepage": "https://github.com/radiocubito/laravel-blade-input",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Jiménez Servín",
            "email": "oliver@radiocubito.com",
            "homepage": "https://radiocubito.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.3",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11",
        "nunomaduro/collision": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Radiocubito\\BladeInput\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radiocubito\\BladeInput\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Radiocubito\\BladeInput\\BladeInputServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
